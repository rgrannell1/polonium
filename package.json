{
  "name": "polonium",
  "version": "0.6.1",
  "bin": {
    "polo": "./src/cli/cli.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rgrannell1/polonium.git"
  },
  "description": "Polonium is a stateless password manager.",
  "engines": {
    "node": ">=5.0.0",
    "npm": ">=3.3.7"
  },
  "scripts": {
    "test": "node test/all.js",
    "standard": "npx standard 'src/**'",
    "standard:fix": "npx standard 'src/**' --fix",
    "snap:install": "cd snapcraft && snap install --dangerous polonium_* && cd ..",
    "snap:snap": "cd snapcraft && snapcraft clean && snapcraft snap && cd .."
  },
  "devDependencies": {
    "standard": "^12.0.1",
    "test": ">=0.0.5"
  },
  "dependencies": {
    "big-integer": "^1.6.11",
    "colors": "^1.1.2",
    "docopt": "^0.6.2",
    "is": "rgrannell1/is",
    "jCheck": "rgrannell1/jCheck",
    "polonium-lib": "github:rgrannell1/polonium-lib",
    "prompt": "^1.0.0",
    "read": "^1.0.7"
  },
  "license": "MIT"
}
